<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @todo_list.title %>
</p>

<p>
  <strong>Description:</strong>
  <%= @todo_list.description %>
</p>

<div id="todo_items_wrapper">
	<%=render @todo_list.todo_items %>
	<div id="form">
		<%= render "todo_items/form" %>
	</div>
</div>

<%= link_to 'Edit', edit_todo_list_path(@todo_list), :class => "btn btn-default" %> |
<%= link_to 'Back', todo_lists_path, :class => "btn btn-default" %>
